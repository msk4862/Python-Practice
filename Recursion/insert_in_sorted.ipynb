   "source": [
    "def insert_in_list(l, item):\n",
    "    if len(l)==0 or l[0]>=item:\n",
    "        l.insert(0, item)\n",
    "        return l\n",
    "    else:\n",
    "        return l[0:1] + insert_in_list(l[1:len(l)], item)"
   ]
