    "def remDup(list1):\n",
    "    if len(list1) == 1:\n",
    "        return list1\n",
    "    elif list1[0]==list1[1]:\n",
    "        return [list1[1]] + remDup(list1[2:])\n",
    "    else:\n",
    "        return [list1[0]] + remDup(list1[1:])"
   ]
